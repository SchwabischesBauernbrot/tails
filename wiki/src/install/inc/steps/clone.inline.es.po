#. type: Plain text
#, no-wrap
msgid ""
"<h1 id=\"install\" class=\"step pc-clone mac-clone\">Install Tails by cloning</h1>\n"
"<h1 id=\"upgrade\" class=\"step upgrade\">Upgrade your Tails by cloning</h1>\n"
msgstr ""
"<h1 id=\"install\" class=\"step pc-clone mac-clone\">Instalar Tails "
"clonando</h1>\n"
"<h1 id=\"upgrade\" class=\"step upgrade\">Actualiza tu Tails clonando</h1>\n"

#. type: Plain text
#, no-wrap
msgid "<div class=\"step-image\">\n"
msgstr "<div class=\"step-image\">\n"

#. type: Plain text
#, no-wrap
msgid "[[!img install/inc/infography/clone-tails.png link=\"no\" class=\"clone upgrade-tails upgrade-os\" alt=\"\"]]\n"
msgstr "[[!img install/inc/infography/clone-tails.png link=\"no\" class=\"clone upgrade-tails upgrade-os\" alt=\"\"]]\n"

#. type: Plain text
#, no-wrap
msgid "</div>\n"
msgstr "</div>\n"

#. type: Plain text
#, fuzzy, no-wrap
#| msgid ""
#| "<p class=\"upgrade\">\n"
#| "  In this step, you will upgrade your Tails from the\n"
#| "  <span class=\"upgrade-clone\">other</span>\n"
#| "  <span class=\"upgrade-tails upgrade-os\">intermediary</span>\n"
#| "  Tails using\n"
#| "  <span class=\"application\">Tails Installer</span>.\n"
#| "</p>\n"
msgid ""
"<p class=\"upgrade\">\n"
"  In this step, you will upgrade your Tails from the\n"
"  <span class=\"upgrade-clone\">other</span>\n"
"  <span class=\"upgrade-tails upgrade-os\">intermediary</span>\n"
"  Tails using\n"
"  <span class=\"application\">Tails Cloner</span>.\n"
"</p>\n"
msgstr ""
"<p class=\"upgrade\">\n"
"  En este paso, actualizarás Tails desde el\n"
"  <span class=\"upgrade-clone\">otro</span> Tails \n"
"  <span class=\"upgrade-tails upgrade-os\">intermedio</span>\n"
"  usando\n"
"  <span class=\"application\">Tails Installer</span>.\n"
"</p>\n"

#. type: Bullet: '1. '
#, fuzzy
#| msgid ""
#| "Plug <span class=\"install-clone mac-clone\">the new</span> <span "
#| "class=\"upgrade\">your Tails</span> USB stick in the computer."
msgid ""
"Plug <span class=\"pc-clone mac-clone\">the new</span> <span "
"class=\"upgrade\">your Tails</span> USB stick into the computer."
msgstr ""
"Conecta <span class=\"install-clone mac-clone\">la nueva</span> <span "
"class=\"upgrade\">tu Tails</span> memoria USB en la computadora."

#. type: Plain text
#, fuzzy, no-wrap
#| msgid ""
#| "   <div class=\"caution install-clone mac-clone\">\n"
#| "   <p>All the data on this USB stick will be lost.</p>\n"
#| "   </div>\n"
msgid ""
"   <div class=\"caution pc-clone mac-clone\">\n"
"   <p>All the data on this USB stick will be lost.</p>\n"
"   </div>\n"
msgstr ""
"   <div class=\"caution install-clone mac-clone\">\n"
"   <p>Toda la información en esta memoria USB se perderá.</p>\n"
"   </div>\n"

