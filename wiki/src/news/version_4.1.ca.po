msgid "[[!meta title=\"Tails 4.1 is out\"]]\n"
msgstr "[[!meta title=\"Tails 4.1\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Tue, 03 Dec 2019 12:34:56 +0000\"]]\n"
msgstr "[[!meta date=\"Tue, 03 Dec 2019 12:34:56 +0000\"]]\n"

